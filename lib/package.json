{
  "name": "ngx-airtable",
  "description": "An Angular module wrapping the Airtable API",
  "version": "2.0.0",
  "homepage": "https://github.com/bohoffi/ngx-airtable",
  "author": {
    "name": "bohoffi",
    "url": "http://github.com/bohoffi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bohoffi/ngx-airtable.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "ng",
    "ngx",
    "angular",
    "library",
    "airtable",
    "API",
    "wrapper"
  ],
  "bugs": {
    "url": "https://github.com/bohoffi/ngx-airtable/issues"
  },
  "main": "bundles/ngx-airtable.umd.js",
  "typings": "index.d.ts",
  "scripts": {
    "cleanup": "rimraf bundles",
    "bundling": "rollup -c ../rollup.config.js",
    "minify": "uglifyjs bundles/ngx-airtable.umd.js --screw-ie8 --compress --mangle --comments --output bundles/ngx-airtable.umd.min.js",
    "build": "npm run cleanup && ngc && npm run bundling && npm run minify",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@types/core-js": "^0.9.41",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rxjs": "^5.0.1",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "uglify-js": "^3.0.10",
    "zone.js": "^0.8.4"
  },
  "peerDependencies": {
    "@angular/common": ">=4.0.0",
    "@angular/core": ">=4.0.0",
    "@angular/http": ">=4.0.0"
  }
}
